#: sistema_pyladies_sp/templates/account/password_change.html:6
#: sistema_pyladies_sp/templates/account/password_change.html:9
#: sistema_pyladies_sp/templates/account/password_change.html:14
#: sistema_pyladies_sp/templates/account/password_reset_from_key.html:5
#: sistema_pyladies_sp/templates/account/password_reset_from_key.html:8
#: sistema_pyladies_sp/templates/account/password_reset_from_key_done.html:4
#: sistema_pyladies_sp/templates/account/password_reset_from_key_done.html:7
msgid "Change Password"
msgstr "Alterar Senha"

#: sistema_pyladies_sp/templates/account/password_reset.html:7
#: sistema_pyladies_sp/templates/account/password_reset.html:11
#: sistema_pyladies_sp/templates/account/password_reset_done.html:6
#: sistema_pyladies_sp/templates/account/password_reset_done.html:9
msgid "Password Reset"
msgstr "Redefinir Senha"

#: sistema_pyladies_sp/templates/account/password_reset.html:16
msgid ""
"Forgotten your password? Enter your e-mail address below, and we'll send you "
"an e-mail allowing you to reset it."
msgstr ""
"Esqueceu sua senha? Informe seu e-mail abaixo e enviaremos um e-mail "
"permitindo que você redefina-a."

#: sistema_pyladies_sp/templates/account/password_reset.html:21
msgid "Reset My Password"
msgstr "Redefinir Minha Senha"

#: sistema_pyladies_sp/templates/account/password_reset.html:24
msgid "Please contact us if you have any trouble resetting your password."
msgstr ""
"Por favor contacte-nos se você tiver qualquer problema redefinindo a senha"

#: sistema_pyladies_sp/templates/account/password_reset_done.html:15
msgid ""
"We have sent you an e-mail. Please contact us if you do not receive it "
"within a few minutes."
msgstr ""
"Enviamos-lhe um e-mail. Por favor contacte-nos se você não recebê-lo em "
"alguns minutos."

#: sistema_pyladies_sp/templates/account/password_reset_from_key.html:8
msgid "Bad Token"
msgstr ""

#: sistema_pyladies_sp/templates/account/password_reset_from_key.html:12
#, python-format
msgid ""
"The password reset link was invalid, possibly because it has already been "
"used.  Please request a <a href=\"%(passwd_reset_url)s\">new password reset</"
"a>."
msgstr ""
"Link para redefinir a senha inválido, possivelmente porque este já foi "
"usado. Por favor <a href=\"%(passwd_reset_url)s\">redefinir senha novamente</"
"a>."

#: sistema_pyladies_sp/templates/account/password_reset_from_key.html:18
msgid "change password"
msgstr "alterar senha"

#: sistema_pyladies_sp/templates/account/password_reset_from_key.html:21
#: sistema_pyladies_sp/templates/account/password_reset_from_key_done.html:8
msgid "Your password is now changed."
msgstr "Sua senha foi alterada."

